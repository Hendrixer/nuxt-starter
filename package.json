{
  "name": "nuxt-starter",
  "version": "0.1.0",
  "description": "A personnal Nuxt starter",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "stylelint src/**/*.scss",
    "format": "prettier --ignore-path .gitignore --write ./**/*.{js,vue}",
    "precommit": "npm run prettier && npm run lint",
    "test": "jest",
    "e2e": "cypress run",
    "e2e:open": "cypress open"
  },
  "author": "Simon Babay <simon@babay.fr>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.42",
    "@nuxtjs/component-cache": "^1.1.1",
    "@nuxtjs/dotenv": "^1.1.1",
    "@nuxtjs/pwa": "^2.0.8",
    "@nuxtjs/router": "^1.0.1",
    "nuxt-class-component": "^1.2.0",
    "nuxt-edge": "^2.0.0-25375479.ba18127",
    "vue-property-decorator": "^6.0.0",
    "vuex-class": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@types/node": "^9.6.1",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "critical": "^1.2.1",
    "cssnano": "^3.10.0",
    "cypress": "^2.1.0",
    "deepmerge": "^2.1.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.4.0",
    "imagemin-webpack-plugin": "^2.1.1",
    "jest": "^22.4.3",
    "jest-vue-preprocessor": "^1.4.0",
    "js-yaml": "^3.11.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.8.3",
    "postcss-import": "^11.1.0",
    "postcss-reporter": "^5.0.0",
    "prettier": "^1.11.1",
    "sass-loader": "^6.0.7",
    "stylelint": "^9.1.3",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.0.0",
    "tota11y": "^0.1.6",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "typescript": "^2.8.1",
    "vue-eslint-parser": "^2.0.3",
    "webpack-bundle-analyzer": "^2.11.1",
    "yaml-loader": "^0.5.0"
  },
  "jest": {
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest",
      "\\.vue$": "<rootDir>/node_modules/jest-vue-preprocessor",
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor"
    },
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "vue-mm": "<rootDir>/internals/modules/manager.js",
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "globals": {
      "__TS_CONFIG__": "tsconfig.json",
      "__TRANSFORM_HTML__": true
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue"
    ],
    "testRegex": "/test/.*\\.(ts|js)$"
  }
}
